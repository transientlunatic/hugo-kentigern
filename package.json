{
  "name": "@transientlunatic/jekyll-kentigern",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "description": "This is the webpack component of the kentigern theme.",
  "main": "index.js",
  "scripts": {
    "develop": "bundle exec jekyll clean && run-p develop:{webpack,jekyll}",
    "develop:jekyll": "sleep 5 && bundle exec jekyll serve --watch --future",
    "develop:webpack": "webpack --mode development --watch",
    "build": "run-s build:webpack build:jekyll",
    "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build",
    "build:webpack": "webpack --mode production"
  },
  "author": "Daniel Williams",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@fontsource-variable/eb-garamond": "^5.2.7",
    "@fontsource/eb-garamond": "^5.2.7",
    "@fontsource/lato": "^5.2.7",
    "@observablehq/plot": "^0.6.16",
    "babel-loader": "^9.1.3",
    "bs5-lightbox": "^1.8.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.5",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.9.4",
    "postcss": "^8.4.41",
    "postcss-loader": "^1.3.3",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "bootstrap": "^5.3.3",
    "file-loader": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "run-p": "^0.0.0",
    "uglify-js": "^3.19.3",
    "url-loader": "^4.1.1"
  }
}
